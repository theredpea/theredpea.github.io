{
    "name": "Click Speed",
    "version": "1.0",
    "description": "Increase processor focus by removing extraneous elements",
    "permissions": [
        "activeTab",
        "declarativeContent",
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://teams.microsoft.com/*"
            ],
            "css": [
                "teams.css"
            ]
        }
    ],
    "manifest_version": 2
}